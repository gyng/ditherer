{
  "license": "MIT",
  "name": "jsapp-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "build": "webpack --progress --config webpack.config.js",
    "lint": "yarn tslint && yarn eslint && yarn stylelint",
    "prettier": "echo 'ðŸ’ˆ Listing files to format. Run yarn prettier:write to write.\n' && prettier --list-different src/**/*.js src/**/*.jsx src/**/*.ts src/**/*.tsx  test/**/*.js test/**/*.jsx *.config.js",
    "prettier:write": "prettier --write src/**/*.js src/**/*.jsx src/**/*.ts src/**/*.tsx test/**/*.js test/**/*.jsx *.config.js",
    "test": "karma start --single-run",
    "test:full": "yarn test && yarn test:functional",
    "test:functional": "mocha test/functional/*.functional.jsx",
    "test:watch": "karma start",
    "flow": "flow check",
    "eslint": "eslint . --ext .js --ext .jsx",
    "eslint:fix": "eslint . --ext .js --ext .jsx --fix",
    "tslint": "tslint -c tslint.json src/**/*.ts src/**/*.tsx",
    "tslint:fix": "tslint -c tslint.json src/**/*.ts src/**/*.tsx --fix",
    "stylelint": "stylelint '**/*.css'",
    "d": "webpack-dev-server --progress --colors"
  },
  "dependencies": {
    "emotion": "^8.0.8",
    "history": "^4.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-emotion": "^8.0.8",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
