    "karma-mocha-reporter": "^2.2.5",
    "karma-nightmare": "^0.4.13",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.5",
    "mocha": "^4.0.1",
    "nightmare": "^2.10.0",
